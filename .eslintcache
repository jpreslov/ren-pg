bel>\r\n              <Select value={shippingOption} fullWidth onChange={(e) => setShippingOption(e.target.value)}>\r\n                {options.map((option) => (\r\n                  <MenuItem key={option.id} value={option.id}>\r\n                    {option.label}\r\n                  </MenuItem>\r\n                ))}\r\n              </Select>\r\n            </Grid>\r\n          </Grid>\r\n          <br />\r\n          <div style={{ display: 'flex', justifyContent: 'space-between' }}>\r\n            <Button component={Link} to=\"/cart\" variant=\"outlined\">\r\n              Back to Cart\r\n            </Button>\r\n            <Button type=\"submit\" variant=\"contained\" color=\"primary\">\r\n              Next\r\n            </Button>\r\n          </div>\r\n        </form>\r\n      </FormProvider>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default AddressForm;\r\n","C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\checkoutform\\CustomTextField.jsx",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\checkoutform\\Review.jsx",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\info\\Info.jsx",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\products\\product\\ProductDetails.jsx",["134","135","136"],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\products\\product\\productDetailsStyles.js",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\info\\styles.js",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\products\\productsStyles.js",[],"C:\\Users\\Jake Lovett\\Desktop\\code\\personal-projects\\ren-pg\\src\\components\\products\\product\\productStyles.js",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":5,"column":10,"nodeType":"143","messageId